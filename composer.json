{
    "name": "doriiaan/filament-menu",
    "description": "A filament menu, inspired by datlechin/filament-menu-builder to reproduce WordPress menu system",
    "keywords": [
        "Doriiaan",
        "laravel",
        "filament-menu"
    ],
    "homepage": "https://github.com/doriiaan/filament-menu",
    "support": {
        "issues": "https://github.com/doriiaan/filament-menu/issues",
        "source": "https://github.com/doriiaan/filament-menu"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Doriiaan",
            "email": "alary.dorian81@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "filament/filament": "^4.0",
        "filament/forms": "^4.0",
        "filament/tables": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "phpstan/phpstan": "^1.11",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Doriiaan\\FilamentMenu\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Doriiaan\\FilamentMenu\\FilamentMenuServiceProvider"
            ],
            "aliases": {
                "FilamentMenu": "Doriiaan\\FilamentMenu\\Facades\\FilamentMenu"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
